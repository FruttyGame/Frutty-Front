{
  "expo": {
    "name": "FruttyGame",
    "slug": "FruttyGame",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mylogo.png",
    "splash": {
      "image": "./assets/mylogo.png",
      "resizeMode": "cover",
      "backgroundColor": "#a51268"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon":{
      "foregroundImage": "./assets/mylogo.png",  
      "backgroundColor": "#a51268"
      }
    }
  }
}
